<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="crudMauiApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:crudMauiApp"
    Shell.FlyoutBehavior="Flyout"
    Title="crudMauiApp">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Maui Crud Page"
        ContentTemplate="{DataTemplate local:MauiCrudPage}"
        Route="MauiCrudPage" />

    <ShellContent
        Title="Web Crud Page"
        ContentTemplate="{DataTemplate local:WebCrudPage}"
        Route="WebCrudPage" />

</Shell>
